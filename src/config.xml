<?xml version="1.0"?>
<MusicClassifier version="1.0">
	
	<Handler name="WavHandler" class="MCModule.FileHandlers.WavHandler" pattern=".*[.]wav$"/>
	
	<Alias name="Hamming" class="MusiC.Extensions.Windows.Hamming"/>
	<Alias name="RollOff" class="MusiC.Extensions.Features.SpecRollOff"/>
	<Alias name="Barbedo" class="MusiC.Extensions.Classifiers.Barbedo"/>
	
	<Train dir="/Users/mjsmagalhaes/projeto_final/data">
		<Label name="classical" input="/Users/mjsmagalhaes/projeto_final/data/classical"/>
		<Label name="electronic" output="/Users/mjsmagalhaes/projeto_final/data/classical"/>
	</Train>
	
	<Classify dir="/Users/mjsmagalhaes/projeto_final/data/classical"/>
 
	<Algorithm>
		
		<Hamming>
			<Param name="Size" value="1024" class="System.Int32"/>
